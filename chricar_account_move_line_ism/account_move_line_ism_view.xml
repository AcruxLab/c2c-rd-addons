<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>
    <!--
==============================================
ISM Mandant
==============================================
-->
    <record model="ir.ui.view" id="ism_mandant_form">
      <field name="name">ism.mandant.form</field>
      <field name="model">ism.mandant</field>
      <field name="priority">1</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <form string="ISM Mandant">
          <field name="name" />
          <field name="company_id" />
          <notebook colspan="4">
            <page string="invisible" invisible="1"/>
          </notebook>
        </form>
      </field>
    </record>
    <record model="ir.ui.view" id="ism_mandant_list">
      <field name="name">ism.mandant.list</field>
      <field name="model">ism.mandant</field>
      <field name="priority">1</field>
      <field name="type">tree</field>
      <field name="arch" type="xml">
        <tree string="ISM Mandant" editable="top">
          <field name="name" />
          <field name="company_id" />
        </tree>
      </field>
    </record>
    <record model="ir.actions.act_window" id="ism_mandant_act1">
      <field name="name">ISM Mandant</field>
      <field name="res_model">ism.mandant</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
      <field name="view_id" ref="ism_mandant_list"/>
    </record>
    <record model="ir.actions.act_window.view" id="ism_mandant_act1_view2">
      <field name="sequence" eval="1"/>
      <field name="view_mode">tree</field>
      <field name="view_id" ref="ism_mandant_list"/>
      <field name="act_window_id" ref="ism_mandant_act1"/>
    </record>
    <record model="ir.actions.act_window.view" id="ism_mandant_act1_view1">
      <field name="sequence" eval="2"/>
      <field name="view_mode">form</field>
      <field name="view_id" ref="ism_mandant_form"/>
      <field name="act_window_id" ref="ism_mandant_act1"/>
    </record>
    <menuitem
      id="menu_ism"
      name="ISM"
      parent="account.menu_finance_entries"
      sequence="150"/>
    <menuitem
      action="ism_mandant_act1"
      id="menu_ism_mandant_list"
      parent="menu_ism"
      sequence="150"/>
    
    
